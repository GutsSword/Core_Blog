

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Core_Blog_Sitesi</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Core_Blog_Sitesi.styles.css" asp-append-version="true" />
   @* <link href="~/cssnew/stylesheet.css" rel="stylesheet" />*@

    <link rel="stylesheet" href="~/standblog/assets/css/fontawesome.css">
    <link rel="stylesheet" href="~/standblog/assets/css/templatemo-stand-blog.css">
    <link rel="stylesheet" href="~/standblog/assets/css/owl.css">


</head>
<body>

        <!-- ***** Preloader Start ***** -->
        <div id="preloader">
            <div class="jumper">
                <div></div>
                <div></div>
                <div></div>
            </div>
        </div>
        <!-- ***** Preloader End ***** -->
        <!-- Header -->
        <header class="">
            <nav class="navbar navbar-expand-lg">
                <div class="container">
                    <a class="navbar-brand" href="/Home/Index/"><h2>Core Blog<em>.</em></h2></a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" style="flex-grow:0" id="navbarResponsive">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item">
                            <a target="_blank" class="nav-link" href="#">
                                <i style="font-size:42px;" class="fa fa-linkedin-square"  aria-hidden="true"></i>
                                    <span class="sr-only"></span>
                                </a>
                            </li>
                            <li class="nav-item">
                            <a target="_blank" class="nav-link" href="about.html"><i style="font-size:42px;" class="fa fa-github" aria-hidden="true"></i></a>
                            </li>
                            <li class="nav-item">
                            <a target="_blank" class="nav-link" href="blog.html"><i style="font-size:42px;" class="fa fa-instagram"  aria-hidden="true"></i></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>

        <!-- Page Content -->
        <!-- Banner Starts Here -->
        <div class="main-banner header-text">
            <div class="container-fluid">
                <div class="owl-banner owl-carousel">
                
                </div>
            </div>
        </div>
        <!-- Banner Ends Here -->
    @RenderBody()

        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="social-icons">
                            <li><a href="#">Facebook</a></li>
                            <li><a href="#">Twitter</a></li>
                            <li><a href="#">Behance</a></li>
                            <li><a href="#">Linkedin</a></li>
                            <li><a href="#">Dribbble</a></li>
                        </ul>
                    </div>
                    <div class="col-lg-12">
                        <div class="copyright-text">
                            <p>
                                Copyright 2020 Stand Blog Co.

                                | Design: <a rel="nofollow" href="https://templatemo.com" target="_parent">TemplateMo</a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

        <!-- Bootstrap core JavaScript -->
        <script src="~/standblog/vendor/jquery/jquery.min.js"></script>
        <script src="~/standblog/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        <!-- Additional Scripts -->
        <script src="~/standblog/assets/js/custom.js"></script>
        <script src="~/standblog/assets/js/owl.js"></script>
        <script src="~/standblog/assets/js/slick.js"></script>
        <script src="~/standblog/assets/js/isotope.js"></script>
        <script src="~/standblog/assets/js/accordions.js"></script>

        <script language="text/Javascript">
            cleared[0] = cleared[1] = cleared[2] = 0; //set a cleared flag for each field
            function clearField(t) {                   //declaring the array outside of the
                if (!cleared[t.id]) {                      // function makes it static and global
                    cleared[t.id] = 1;  // you could use true and false, but that's more typing
                    t.value = '';         // with more chance of typos
                    t.style.color = '#fff';
                }
            }
        </script>

    </body>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)

</html>
